{
    "window.commandCenter": true,
    "cursor.diffs.useCharacterLevelDiffs": true,
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "vim.enabled": true,
    "vim.useSystemClipboard": true,
    "workbench.iconTheme": "catppuccin noctis icons",
    "sync.autoUpload": true,
    "editor.background": "#000000",
    "editor.semanticHighlighting.enabled": true,
    "editor.lineNumbers": "relative",
    "vim.leader": "<space>",
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "rgba(250, 240, 170, 0.5)",
    "vim.highlightedyank.duration": 200,
    "vim.handleKeys": {
        "<C-d>": true,
        "<C-u>": true,
        "<C-f>": true,
        "<C-b>": true,
        "<C-h>": true,
        "<C-j>": true,
        "<C-k>": true,
        "<C-l>": true
    },
    "vim.normalModeKeyBindings": [
        {
            "before": ["<C-h>"],
            "commands": ["workbench.action.previousEditor"]
        },
        {
            "before": ["<C-l>"],
            "commands": ["workbench.action.nextEditor"]
        },
        {
            "before": ["<C-w>", "h"],
            "commands": ["workbench.action.focusPreviousGroup"]
        },
        {
            "before": ["<C-w>", "l"],
            "commands": ["workbench.action.focusNextGroup"]
        },
        {
            "before": ["shift", "k"],
            "commands": ["editor.action.showHover"]
        },
        {
            "before": ["leader", "g", "d"],
            "commands": ["editor.action.revealDefinition"]
        },
        {
            "before": ["leader", "r", "n"],
            "commands": ["editor.action.rename"]
        },
        {
            "before": ["leader", "c", "a"],
            "commands": ["editor.action.codeAction"]
        },
        {
            "before": ["leader", "f", "d"],
            "commands": ["editor.action.goToReferences"]
        },
        {
            "before": ["leader", "f", "m"],
            "commands": ["workbench.action.gotoSymbol"]
        },
        {
            "before": ["leader", "f", "g"],
            "commands": ["workbench.action.quickTextSearch"]
        },
        {
            "before": ["leader", "c", "c"],
            "commands": ["workbench.action.toggleAuxiliaryBar"]
        },
        {
            "before": ["leader", "e", "e"],
            "commands": ["workbench.action.toggleSidebarVisibility"],
        },
        {
            "before": ["leader", "e", "e"],
            "commands": ["workbench.action.toggleSidebarVisibility"],
            "when": "sideBarVisible",
        },
        {
            "before": ["leader", "a"],
            "commands": ["vscode-harpoon.addEditor"]
        },
        {
            "before": ["leader", "p", "e"],
            "commands": ["vscode-harpoon.editorQuickPick"]
        },
        {
            "before": ["leader", "1"],
            "commands": ["vscode-harpoon.gotoEditor1"]
        },
        {
            "before": ["leader", "2"],
            "commands": ["vscode-harpoon.gotoEditor2"]
        },
        {
            "before": ["leader", "3"],
            "commands": ["vscode-harpoon.gotoEditor3"]
        },
        {
            "before": ["leader", "4"],
            "commands": ["vscode-harpoon.gotoEditor4"]
        },
        {
            "before": ["leader", "g", "i"],
            "commands": ["editor.action.goToImplementation"]
        },
        {
            "before": ["K"],
            "commands": ["editor.action.showHover"]
        },
        {
            "before": ["<C-p>"],
            "commands": ["workbench.action.quickOpen"]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["K"],
            "commands": ["editor.action.moveLinesUpAction"]
        },
        {
            "before": ["J"],
            "commands": ["editor.action.moveLinesDownAction"]
        }
    ],
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 6,
    "workbench.colorTheme": "Catppuccin Mocha",
    "go.toolsManagement.autoUpdate": true,
    "extensions.ignoreRecommendations": true,
    "githubLocalActions.dockerDesktopPath": "/Applications/Docker.app",
    "catppuccin-noctis-icons.hidesExplorerArrows": false,
    "editor.fontFamily": "JetBrainsMono Nerd Font",
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "bash",
            "args": [
                "-l"
            ],
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "zsh",
            "args": [
                "-l"
            ]
        },
        "fish": {
            "path": "fish",
            "args": [
                "-l"
            ]
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        }
    },
    "terminal.integrated.defaultProfile.osx": "bash",
    "workbench.list.automaticKeyboardNavigation": false,
    "workbench.list.keyboardNavigation": "simple",
    "vim.insertModeKeyBindings": [
        {
            "before": ["<C-j>"],
            "commands": ["selectNextSuggestion"],
            "when": "suggestWidgetVisible"
        },
        {
            "before": ["<C-k>"],
            "commands": ["selectPrevSuggestion"],
            "when": "suggestWidgetVisible"
        }
    ]
}
